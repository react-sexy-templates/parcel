{
  "name": "parcel",
  "version": "0.0.1",
  "description": "A react + parcel template",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development parcel ./src/index.html -p 8080",
    "build": "cross-env NODE_ENV=production parcel build -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-sexy-templates/parcel.git"
  },
  "keywords": [
    "react",
    "parcel",
    "react-sexy"
  ],
  "author": "JeLewine",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/react-sexy-templates/parcel/issues"
  },
  "homepage": "https://github.com/react-sexy-templates/parcel#readme",
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.3",
    "parcel-bundler": "^1.5.1"
  },
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {
        "browsers": [">1%", "last 4 versions", "Firefox ESR", "not ie < 9"],
        "flexbox": "no-2009"
      }
    }
  }
}
